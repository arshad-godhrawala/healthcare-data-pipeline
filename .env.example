# Healthcare Data Pipeline Environment Variables
# Copy this file to .env and update with your actual values

# PostgreSQL Database Configuration
POSTGRES_USER=healthcare_user
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=healthcare_db
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# InfluxDB Configuration
INFLUXDB_USER=admin
INFLUXDB_PASSWORD=your_secure_influxdb_password
INFLUXDB_ORG=healthcare_org
INFLUXDB_BUCKET=vital_signs
INFLUXDB_TOKEN=your_influxdb_admin_token_here

# API Configuration
API_HOST=localhost
API_PORT=8000
DEBUG=True

# Dashboard Configuration
DASHBOARD_PORT=3000

# Security (Generate secure keys in production)
SECRET_KEY=your_secret_key_here
ENCRYPTION_KEY=your_encryption_key_here

# Application Settings
ENVIRONMENT=development
LOG_LEVEL=INFO

# Optional: Cloud Storage (if using AWS S3)
# AWS_ACCESS_KEY_ID=your_aws_access_key
# AWS_SECRET_ACCESS_KEY=your_aws_secret_key
# S3_BUCKET_NAME=healthcare-data-bucket

# Optional: Email Notifications
# SMTP_SERVER=your_smtp_server
# SMTP_PORT=587
# EMAIL_USER=your_email@domain.com
# EMAIL_PASSWORD=your_email_password